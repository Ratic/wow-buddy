{% extends "XRealmAppBundle:Includes:base_layout.html.twig" %}

{% block page %}
    <div class="container">
    <h1>{{ "message.externallink"|trans }}</h1>
    <p>
    {{ "message.externallink_description"|trans }}
    </p>
    <h3>{{ "website.target"|trans }}</h3>
    <pre>{{ url }}</pre>
    <a class="btn btn-primary" href="{{ url }}">{{ "website.next"|trans }}</a>
    <a class="btn btn-default" href="javascript:close();">{{ "website.close"|trans }}</a>
    </div>

{% endblock page %}